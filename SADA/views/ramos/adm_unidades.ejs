<%include ../layout %>
<%include ../nav %>

<div class="menu container">
  <div class="row">
    <section class="col-sm-4 ramos">
      <h4>Unidad: <label> <%=ramo_nombre%></label></h4>
      <%for(var i=0;i<unidades.length;i++){%>
        <form action="/adm_plantilla" method="post">
          <input type="hidden" value=<%=unidades[i].idUnidad%> name="unidad_id">
          <input type="hidden" value=<%=unidades[i].Nombre%> name="unidad_nombre">
          <button type="submit"><i class="fa fa-wrench" aria-hidden="true"></i> <%=unidades[i].Nombre%></button>
        </form>
      <%}%>
    </section>

  </div> <!-- end row -->
</div>

